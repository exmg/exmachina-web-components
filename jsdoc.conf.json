{
  "plugins": [
    "plugins/markdown"
  ],
  "recurseDepth": 10,
  "opts": {
    "template": "./node_modules/tui-jsdoc-template",
    "destination": "./docs",
    "recurse": true
  },
  "source": {
    "include": [
      "packages/exmg-breadcrumbs",
      "packages/exmg-button",
      "packages/exmg-collapsed",
      "packages/exmg-copy-to-clipboard",
      "packages/exmg-dialogs",
      "packages/exmg-form",
      "packages/exmg-form-drawer",
      "packages/exmg-grid",
      "packages/exmg-markdown-editor",
      "packages/exmg-radio-group",
      "packages/exmg-sidemenu",
      "packages/exmg-snckbar",
      "packages/exmg-sortable",
      "packages/exmg-tooltip"
    ]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "templates": {
    "logo": {
      "url": "https://www.exmachinagroup.com/images/svg/ExMachinaPrimary.svg",
      "width": "150px",
      "height": "50px",
      "link": ""
    },
    "cleverLinks": false,
    "monospaceLinks": false,
    "name": "",
    "footerText": "",
    "useCollapsibles": true
  }
}