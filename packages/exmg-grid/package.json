{
  "name": "@exmg/exmg-grid",
  "version": "8.0.0",
  "description": "exmg grid element",
  "contributors": [
    "Ex Machina"
  ],
  "keywords": [
    "web-components",
    "polymer",
    "lit-element",
    "grid"
  ],
  "main": "index.js",
  "files": [
    "assets/*",
    "src/**/*.scss",
    "src/**/exmg-*.js",
    "src/**/exmg-*.d.ts",
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ExmgElements/exmg-elements-monorepo.git"
  },
  "license": "MIT",
  "dependencies": {
    "@exmg/exmg-sortable": "^8.0.0",
    "@material/mwc-menu": "^0.25.1",
    "@material/mwc-button": "^0.25.1",
    "@material/mwc-list": "^0.25.1",
    "@material/mwc-base": "^0.25.1",
    "@material/mwc-checkbox": "^0.25.1",
    "@material/mwc-icon-button": "^0.25.1",
    "@material/theme": "^12.0.0",
    "@material/typography": "^12.0.0",
    "@polymer/iron-dropdown": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-input": "^3.2.1",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/polymer": "^3.4.1",
    "lit": "^2.0.0"
  },
  "devDependencies": {
    "@exmg/exmg-cli": "^4.1.0",
    "@web/dev-server": "0.0.29",
    "concurrently": "^6.2.1",
    "@polymer/iron-component-page": "^4.0.1",
    "@polymer/iron-demo-helpers": "^3.1.0",
    "@polymer/paper-menu-button": "^3.0.1",
    "@polymer/test-fixture": "^4.0.2",
    "@types/chai": "^4.2.11",
    "@types/material-components-web": "^1.0.0",
    "@types/mocha": "^5.2.6",
    "@types/polymer": "^1.2.9",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "chai": "^4.2.0",
    "eslint": "^7.8.1",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^6.2.3",
    "typedoc": "^0.19.0",
    "typescript": "^4.0.2",
    "wct-mocha": "^1.1.0",
    "web-animations-js": "^2.3.1"
  },
  "scripts": {
    "package-install": "npm i",
    "fix-gen-docs": "node ./node_modules/@exmg/exmg-cli/src/gen-docs/bin/gen-docs.js ./ -d ./docs -a",
    "gen-docs": "typedoc --excludeNotExported --excludeExternals --excludePrivate --excludeProtected --ignoreCompilerErrors --exclude '{**/*test*,**/node_modules/**,**/test/**}' --out docs/ src/**/*.ts && npm run fix-gen-docs",
    "tsc:watch": "tsc -b ./tsconfig.json --watch",
    "tsc": "tsc -b ./tsconfig.json",
    "lint": "tsc --noEmit && eslint '**/*ts' --ignore-path ../../.eslintignore -c ../../eslintrc.json --quiet --fix",
    "build-styles": "exmg-cli -s \"*/styles/**/*.scss\"",
    "watch-styles": "exmg-cli -w",
    "build": "npm run build-styles && npm run lint && npm run tsc",
    "prepublishOnly": "npm run build",
    "preversion": "npm run build",
    "publishDryRun": "npm publish --dry-run",
    "start": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --app-index demo/index.html --node-resolve --open --watch\"",
    "test-dev": "polymer test --npm -p",
    "test": "polymer test --npm --expanded"
  },
  "publishConfig": {
    "access": "public"
  }
}
