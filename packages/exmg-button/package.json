{
  "name": "@exmg/exmg-button",
  "version": "2.0.2",
  "dependencies": {
    "@material/mwc-button": "^0.18.0",
    "@material/typography": "^7.0.0",
    "lit-element": "^2.4.0"
  },
  "keywords": [
    "web-components",
    "polymer",
    "lit-element",
    "button"
  ],
  "main": "index.js",
  "files": [
    "styles/**/*.scss",
    "exmg-*.js",
    "exmg-*.d.ts",
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ExmgElements/exmg-elements-monorepo.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@exmg/exmg-cli": "^2.1.0",
    "@material/mwc-checkbox": "^0.18.0",
    "@material/mwc-icon": "^0.18.0",
    "@polymer/iron-component-page": "^4.0.1",
    "@polymer/iron-demo-helpers": "^3.1.0",
    "@polymer/test-fixture": "^4.0.2",
    "@types/chai": "^4.2.11",
    "@types/material-components-web": "^1.0.0",
    "@types/mocha": "^5.2.6",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "chai": "^4.2.0",
    "eslint": "^7.8.1",
    "eslint-plugin-lit": "^1.2.2",
    "gulp": "^4.0.0",
    "gulp-watch": "^5.0.1",
    "mocha": "^5.2.0",
    "typedoc": "^0.19.0",
    "typescript": "^4.0.3",
    "wct-mocha": "^1.1.0"
  },
  "scripts": {
    "tsc:watch": "tsc --watch",
    "tsc": "tsc --project ./tsconfig.json",
    "lint": "tsc --noEmit && eslint '**/*ts' --ignore-path ../../.eslintignore -c ../../eslintrc.json --quiet --fix",
    "build:styles": "gulp render-styles",
    "build": "npm run build:styles && npm run lint && npm run tsc",
    "prepublishOnly": "npm run build",
    "preversion": "npm run build",
    "publishDryRun": "npm publish --dry-run",
    "start": "npm run build:styles && gulp serve",
    "test-dev": "polymer test --npm -p",
    "test": "polymer test --npm --expanded"
  },
  "publishConfig": {
    "access": "public"
  }
}