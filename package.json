{
  "name": "exmachina-web-components",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@exmg/exmg-cli": "^4.0.0",
    "@material/typography": "^12.0.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/web-components": "^6.2.9",
    "@web/dev-server": "0.0.29",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "lerna": "^4.0.0",
    "lit": "^2.0.0-rc.2",
    "lit-html": "^1.4.1",
    "typescript": "^4.3.2"
  },
  "scripts": {
    "start": "npm run build:styles && node ./scripts/parseElements.js && concurrently --names tsc  \"npm run storybook\" \"npm run watch\" \"npm run watch:demos\" \"web-dev-server --config ./web-dev-server.config.js\"",
    "watch": "exmg-cli -w",
    "watch:demos": "tsc -b -w",
    "tsc": "lerna run tsc",
    "build:styles": "exmg-cli -s \"packages/**/src/styles/*.scss\"",
    "bootstrap": "./scripts/bootstrap.sh",
    "postinstall": "npm run bootstrap",
    "storybook": "start-storybook --quiet -p 6006",
    "build-storybook": "build-storybook",
    "publish": "lerna run tsc && lerna publish"
  },
  "dependencies": {
    "storybook": "^6.2.9"
  }
}