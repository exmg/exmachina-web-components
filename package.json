{
  "version": "8.1.0",
  "name": "exmachina-web-components",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "@babel/traverse": "^7.15.4",
    "@exmg/lit-cli": "1.1.9",
    "@material/typography": "^13.0.0",
    "@polymer/iron-demo-helpers": "^3.1.0",
    "@polymer/polymer": "^3.4.1",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-strip": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "@web/dev-server": "0.0.29",
    "@webcomponents/shadycss": "^1.11.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "babel-loader": "^8.2.2",
    "babylon": "^6.18.0",
    "concurrently": "^6.2.0",
    "del-cli": "^3.0.1",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-wc": "^1.2.0",
    "jsdoc": "^3.6.10",
    "lerna": "^4.0.0",
    "lit": "^2.0.0",
    "lit-analyzer": "^1.2.1",
    "rollup": "^2.57.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-terser": "^7.0.2",
    "run-script-os": "^1.1.6",
    "ts-lit-plugin": "^1.2.1",
    "tui-jsdoc-template": "^1.2.2",
    "typescript": "4.3.2",
    "web-animations-js": "^2.3.1"
  },
  "scripts": {
    "clean": "del-cli packages/*/node_modules packages/*/package-lock.json && npm run clean:styles & npm run clean:typescript",
    "clean:styles": "del-cli packages/**/*-css.*",
    "clean:typescript": "tsc --build --clean",
    "lint:imports": "node scripts/check-imports.js",
    "lint": "npm run lint:lit",
    "lint:fix": "npm run lint:typescript -- --fix && npm run lint:lit -- --fix",
    "lint:typescript": "eslint \"packages/**/*.ts\"",
    "lint:lit": "lit-analyzer \"packages/**/*.ts\" --strict",
    "start": "node ./scripts/parseElements.js && npm run build && concurrently --names watch,serve \"npm run watch\" \"web-dev-server --config ./web-dev-server.config.js\"",
    "watch": "node scripts/watcher.js",
    "watch:styles": "exmg-lit-cli sass",
    "build": "npm run build:styling && npm run build:typescript",
    "build:typescript": "tsc --build --verbose",
    "build:demo-styles": "exmg-lit-cli sass -f \"demo/**/*.scss\"",
    "build:styling": "lerna run --stream build:styles && npm run build:demo-styles",
    "bootstrap": "run-script-os",
    "bootstrap:windows": "./scripts/bootstrap-win.sh",
    "bootstrap:default": "./scripts/bootstrap.sh",
    "postinstall": "npm run bootstrap",
    "version": "lerna version",
    "build:demo": "node ./scripts/parseElements.js && npm run build && rollup --config ./rollup.config.js",
    "run:demo": "web-dev-server ./docs/demo/index.html",
    "build:docs": "npm run build && rm -rf ./docs && npm run build:demo && ./node_modules/.bin/jsdoc -c ./jsdoc.conf.json -R README.md && node ./scripts/processDemoInDocumentation.js",
    "run:docs": "web-dev-server /docs/index.html"
  },
  "dependencies": {
    "@open-wc/building-rollup": "^2.0.2",
    "@vaadin/vaadin-date-picker": "^23.2.2"
  }
}
